{
  "name": "cozy-konnector-facebook",
  "version": "1.2.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-facebook.git"
  },
  "keywords": [],
  "author": "Cozy Cloud",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "eslintConfig": {
    "extends": [
      "eslint-config-cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "scripts": {
    "start": "node ./src/index.js",
    "token": "node ./facebookHelper.js",
    "dev": "cozy-run-dev",
    "standalone": "cozy-run-standalone",
    "pretest": "npm run clean",
    "test": "konitor testit .",
    "check": "konitor check .",
    "clean": "rm -rf ./data",
    "build": "webpack",
    "precommit": "yarn lint",
    "lint": "eslint --fix .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/konnectors/cozy-konnector-facebook.git}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "travisDeployKey": "./bin/generate_travis_deploy_key"
  },
  "dependencies": {
    "cozy-konnector-libs": "4.15.1",
    "fb": "2.0.0"
  },
  "devDependencies": {
    "chalk": "2.4.2",
    "clear": "0.1.0",
    "configstore": "4.0.0",
    "copy-webpack-plugin": "4.6.0",
    "cozy-app-publish": "0.15.0",
    "cozy-jobs-cli": "1.8.2",
    "eslint": "5.15.1",
    "eslint-config-cozy-app": "1.1.12",
    "figlet": "1.2.1",
    "git-directory-deploy": "1.5.1",
    "husky": "1.3.1",
    "konitor": "0.10.2",
    "minimist": "1.2.0",
    "svgo": "1.2.0",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0"
  }
}
